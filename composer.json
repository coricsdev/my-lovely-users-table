{
    "name": "coricsdev/my-lovely-users-table",
    "description": "A WordPress plugin for displaying a table of users via API.",
    "type": "wordpress-plugin",
    "authors": [
        {
            "name": "Rico Dadiz",
            "email": "dadizrico@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "inpsyde/php-coding-standards": "^2@dev",
        "phpcompatibility/php-compatibility": "^10.0",
        "squizlabs/php_codesniffer": "^3.6",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3",
        "brain/monkey": "^2.6",
        "antecedent/patchwork": "^2.1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "MyLovelyUsersTable\\": "includes/"
        }
    },
    "autoload-dev": {
        "psr-4": { "MyLovelyUsersTable\\Tests\\": "tests/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
